        ROUND(AVG(sm2zone01_101), 4) AS avg_sm2zone01_101, ROUND(AVG(sm2zone01_102), 4) AS avg_sm2zone01_102, ROUND(AVG(sm2zone01_103), 4) AS avg_sm2zone01_103, ROUND(AVG(sm2zone01_104), 4) AS avg_sm2zone01_104, ROUND(AVG(sm2zone01_105), 4) AS avg_sm2zone01_105, ROUND(AVG(sm2zone01_106), 4) AS avg_sm2zone01_106, ROUND(AVG(sm2zone01_107), 4) AS avg_sm2zone01_107, ROUND(AVG(sm2zone01_108), 4) AS avg_sm2zone01_108, ROUND(AVG(sm2zone01_109), 4) AS avg_sm2zone01_109, ROUND(AVG(sm2zone01_110), 4) AS avg_sm2zone01_110,
        ROUND(AVG(sm2zone01_111), 4) AS avg_sm2zone01_111, ROUND(AVG(sm2zone01_112), 4) AS avg_sm2zone01_112, ROUND(AVG(sm2zone01_113), 4) AS avg_sm2zone01_113, ROUND(AVG(sm2zone01_114), 4) AS avg_sm2zone01_114, ROUND(AVG(sm2zone01_115), 4) AS avg_sm2zone01_115, ROUND(AVG(sm2zone01_116), 4) AS avg_sm2zone01_116, ROUND(AVG(sm2zone01_117), 4) AS avg_sm2zone01_117, ROUND(AVG(sm2zone01_118), 4) AS avg_sm2zone01_118, ROUND(AVG(sm2zone01_119), 4) AS avg_sm2zone01_119, ROUND(AVG(sm2zone01_120), 4) AS avg_sm2zone01_120,
        ROUND(AVG(sm2zone01_121), 4) AS avg_sm2zone01_121, ROUND(AVG(sm2zone01_122), 4) AS avg_sm2zone01_122, ROUND(AVG(sm2zone01_123), 4) AS avg_sm2zone01_123, ROUND(AVG(sm2zone01_124), 4) AS avg_sm2zone01_124, ROUND(AVG(sm2zone01_125), 4) AS avg_sm2zone01_125, ROUND(AVG(sm2zone01_126), 4) AS avg_sm2zone01_126, ROUND(AVG(sm2zone01_127), 4) AS avg_sm2zone01_127, ROUND(AVG(sm2zone01_128), 4) AS avg_sm2zone01_128, ROUND(AVG(sm2zone01_129), 4) AS avg_sm2zone01_129, ROUND(AVG(sm2zone01_130), 4) AS avg_sm2zone01_130,
        ROUND(AVG(sm2zone01_131), 4) AS avg_sm2zone01_131, ROUND(AVG(sm2zone01_132), 4) AS avg_sm2zone01_132, ROUND(AVG(sm2zone01_133), 4) AS avg_sm2zone01_133, ROUND(AVG(sm2zone01_134), 4) AS avg_sm2zone01_134, ROUND(AVG(sm2zone01_135), 4) AS avg_sm2zone01_135, ROUND(AVG(sm2zone01_136), 4) AS avg_sm2zone01_136, ROUND(AVG(sm2zone01_137), 4) AS avg_sm2zone01_137, ROUND(AVG(sm2zone01_138), 4) AS avg_sm2zone01_138, ROUND(AVG(sm2zone01_139), 4) AS avg_sm2zone01_139, ROUND(AVG(sm2zone01_140), 4) AS avg_sm2zone01_140,

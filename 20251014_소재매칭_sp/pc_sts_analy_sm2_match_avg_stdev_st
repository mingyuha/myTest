        std_dev_sm2zone05_1, std_dev_sm2zone05_2, std_dev_sm2zone05_3, std_dev_sm2zone05_4, std_dev_sm2zone05_5, std_dev_sm2zone05_6, std_dev_sm2zone05_7, std_dev_sm2zone05_8, std_dev_sm2zone05_9, std_dev_sm2zone05_10,
        std_dev_sm2zone05_11, std_dev_sm2zone05_12, std_dev_sm2zone05_13, std_dev_sm2zone05_14, std_dev_sm2zone05_15, std_dev_sm2zone05_16, std_dev_sm2zone05_17, std_dev_sm2zone05_18, std_dev_sm2zone05_19, std_dev_sm2zone05_20,
        std_dev_sm2zone05_21, std_dev_sm2zone05_22, std_dev_sm2zone05_23, std_dev_sm2zone05_24, std_dev_sm2zone05_25, std_dev_sm2zone05_26, std_dev_sm2zone05_27, std_dev_sm2zone05_28, std_dev_sm2zone05_29, std_dev_sm2zone05_30
    )
    WITH T1 AS (
        SELECT
            t1.heat_no,
            t3.step_seq_no,
            t1.step_cd,
            t1.rework_seq_no,
            DATETIME_TRUNC(MIN(t1.entry_dtm), SECOND) entry_dtm,
            DATETIME_TRUNC(MAX(t1.exit_dtm), SECOND) exit_dtm,
            t2.zone_id,
            t1.zone_nm,
            t1.subzone_nm
        FROM `sm2_l2.sm2_zone_mid_trk_mt` t1
        LEFT JOIN `sm2_l2.sm2_zone_info_mt` t2 ON t1.subzone_nm=t2.zone_nm
        LEFT JOIN `sm2_l2.sm2_step_info_mt` t3 ON t1.subzone_nm=t3.zone_nm AND t1.step_cd=t3.step_cd
        WHERE (1=1)
        AND t1.abnorm_yn IN ('n', 'N')
        AND t1.entry_dtm IS NOT NULL
        AND t1.exit_dtm IS NOT NULL
        AND t1.entry_dtm < t1.exit_dtm
        AND DATE(t1.inout_dtm) BETWEEN DATE_SUB(DATE(p_datetime), INTERVAL 1 DAY) AND DATE_ADD(DATE(p_datetime), INTERVAL 1 DAY)
        AND DATE(t1.entry_dtm) >= DATE(p_datetime)
        AND DATE(t1.entry_dtm) < DATE_ADD(DATE(p_datetime), INTERVAL 1 DAY)
        GROUP BY t1.heat_no, t3.step_seq_no, t1.step_cd, t1.rework_seq_no, t2.zone_id, t1.zone_nm, t1.subzone_nm, t2.zone_cd
    ), T2 AS (
        SELECT *
        FROM (
            SELECT
                heat_no, cssteelkindname,
                ROW_NUMBER() OVER(PARTITION BY heat_no ORDER BY trktime DESC) AS row_num
            FROM dataforge-seahcss.sm2_l2.sm2_workorder_mt
            WHERE
                DATE(trktime) BETWEEN DATE_SUB(DATE(p_datetime), INTERVAL 1 YEAR) AND DATE_ADD(DATE(p_datetime), INTERVAL 1 YEAR) AND
                cssteelkindname in ('316LDS1', 'STS410S5','STS304SX','STS303S1','316LDSZ')
        )
        WHERE row_num = 1
    ), T3 AS (
        SELECT DATETIME(stats_sec_dtm) AS stats_sec_dtm
        FROM UNNEST(GENERATE_TIMESTAMP_ARRAY(
            TIMESTAMP(p_datetime),
            TIMESTAMP_ADD(TIMESTAMP(p_datetime), INTERVAL 1 DAY),
            INTERVAL 1 SECOND)) AS stats_sec_dtm
    ), Z1 AS (
        SELECT 'EAF' zone_nm, *
        FROM sm2_tag.tag_sec_zone01_st
        WHERE stats_sec_dtm >= p_datetime
        AND stats_sec_dtm < DATETIME_ADD(p_datetime, INTERVAL 1 DAY)
    ), Z2 AS (
        SELECT 'LF' zone_nm, *
        FROM sm2_tag.tag_sec_zone02_st
        WHERE stats_sec_dtm >= p_datetime
        AND stats_sec_dtm < DATETIME_ADD(p_datetime, INTERVAL 1 DAY)
    ), Z3 AS (
        SELECT 'VD' zone_nm, *
        FROM sm2_tag.tag_sec_zone03_st
        WHERE stats_sec_dtm >= p_datetime
        AND stats_sec_dtm < DATETIME_ADD(p_datetime, INTERVAL 1 DAY)
    ), Z4 AS (
        SELECT 'AOD' zone_nm, *
        FROM sm2_tag.tag_sec_zone04_st
        WHERE stats_sec_dtm >= p_datetime
        AND stats_sec_dtm < DATETIME_ADD(p_datetime, INTERVAL 1 DAY)
    ), Z5 AS (
        SELECT 'LTS' zone_nm, *
        FROM sm2_tag.tag_sec_zone05_st
        WHERE stats_sec_dtm >= p_datetime
        AND stats_sec_dtm < DATETIME_ADD(p_datetime, INTERVAL 1 DAY)
    ), T9 AS (
        SELECT
            T1.heat_no,
            CAST(T1.step_seq_no AS STRING) step_seq_no,
            T1.step_cd,
            CAST(T1.rework_seq_no AS STRING) rework_seq_no,
            CAST(T1.zone_id AS STRING) zone_id,
            T1.zone_nm,
            T1.subzone_nm,
            T1.entry_dtm,
            T1.exit_dtm,
            T2.cssteelkindname,
            T3.stats_sec_dtm,
            Z1.* EXCEPT(zone_nm, stats_sec_dtm),
            Z2.* EXCEPT(zone_nm, stats_sec_dtm),
            Z3.* EXCEPT(zone_nm, stats_sec_dtm),
            Z4.* EXCEPT(zone_nm, stats_sec_dtm),
            Z5.* EXCEPT(zone_nm, stats_sec_dtm)
        FROM T1
        LEFT JOIN T2 t2 ON t1.heat_no=t2.heat_no
        LEFT JOIN T3 ON T3.stats_sec_dtm BETWEEN T1.entry_dtm AND T1.exit_dtm
        LEFT JOIN Z1 ON T1.zone_nm=Z1.zone_nm AND T3.stats_sec_dtm=Z1.stats_sec_dtm
        LEFT JOIN Z2 ON T1.zone_nm=Z2.zone_nm AND T3.stats_sec_dtm=Z2.stats_sec_dtm
        LEFT JOIN Z3 ON T1.zone_nm=Z3.zone_nm AND T3.stats_sec_dtm=Z3.stats_sec_dtm
        LEFT JOIN Z4 ON T1.zone_nm=Z4.zone_nm AND T3.stats_sec_dtm=Z4.stats_sec_dtm
        LEFT JOIN Z5 ON T1.zone_nm=Z5.zone_nm AND T3.stats_sec_dtm=Z5.stats_sec_dtm
        WHERE NOT (
            Z1.stats_sec_dtm IS NULL
            AND Z2.stats_sec_dtm IS NULL
            AND Z3.stats_sec_dtm IS NULL
            AND Z4.stats_sec_dtm IS NULL
            AND Z5.stats_sec_dtm IS NULL
        )
    )
    SELECT
        heat_no,
        step_seq_no,
        step_cd,
        rework_seq_no,
        zone_id,
        zone_nm,
        subzone_nm,
        entry_dtm,
        exit_dtm,
        cssteelkindname,
        ROUND(AVG(sm2zone01_1), 4) AS avg_sm2zone01_1, ROUND(AVG(sm2zone01_2), 4) AS avg_sm2zone01_2, ROUND(AVG(sm2zone01_3), 4) AS avg_sm2zone01_3, ROUND(AVG(sm2zone01_4), 4) AS avg_sm2zone01_4, ROUND(AVG(sm2zone01_5), 4) AS avg_sm2zone01_5, ROUND(AVG(sm2zone01_6), 4) AS avg_sm2zone01_6, ROUND(AVG(sm2zone01_7), 4) AS avg_sm2zone01_7, ROUND(AVG(sm2zone01_8), 4) AS avg_sm2zone01_8, ROUND(AVG(sm2zone01_9), 4) AS avg_sm2zone01_9, ROUND(AVG(sm2zone01_10), 4) AS avg_sm2zone01_10,
        ROUND(AVG(sm2zone01_11), 4) AS avg_sm2zone01_11, ROUND(AVG(sm2zone01_12), 4) AS avg_sm2zone01_12, ROUND(AVG(sm2zone01_13), 4) AS avg_sm2zone01_13, ROUND(AVG(sm2zone01_14), 4) AS avg_sm2zone01_14, ROUND(AVG(sm2zone01_15), 4) AS avg_sm2zone01_15, ROUND(AVG(sm2zone01_16), 4) AS avg_sm2zone01_16, ROUND(AVG(sm2zone01_17), 4) AS avg_sm2zone01_17, ROUND(AVG(sm2zone01_18), 4) AS avg_sm2zone01_18, ROUND(AVG(sm2zone01_19), 4) AS avg_sm2zone01_19, ROUND(AVG(sm2zone01_20), 4) AS avg_sm2zone01_20,
        ROUND(AVG(sm2zone01_21), 4) AS avg_sm2zone01_21, ROUND(AVG(sm2zone01_22), 4) AS avg_sm2zone01_22, ROUND(AVG(sm2zone01_23), 4) AS avg_sm2zone01_23, ROUND(AVG(sm2zone01_24), 4) AS avg_sm2zone01_24, ROUND(AVG(sm2zone01_25), 4) AS avg_sm2zone01_25, ROUND(AVG(sm2zone01_26), 4) AS avg_sm2zone01_26, ROUND(AVG(sm2zone01_27), 4) AS avg_sm2zone01_27, ROUND(AVG(sm2zone01_28), 4) AS avg_sm2zone01_28, ROUND(AVG(sm2zone01_29), 4) AS avg_sm2zone01_29, ROUND(AVG(sm2zone01_30), 4) AS avg_sm2zone01_30,
        ROUND(AVG(sm2zone01_31), 4) AS avg_sm2zone01_31, ROUND(AVG(sm2zone01_32), 4) AS avg_sm2zone01_32, ROUND(AVG(sm2zone01_33), 4) AS avg_sm2zone01_33, ROUND(AVG(sm2zone01_34), 4) AS avg_sm2zone01_34, ROUND(AVG(sm2zone01_35), 4) AS avg_sm2zone01_35, ROUND(AVG(sm2zone01_36), 4) AS avg_sm2zone01_36, ROUND(AVG(sm2zone01_37), 4) AS avg_sm2zone01_37, ROUND(AVG(sm2zone01_38), 4) AS avg_sm2zone01_38, ROUND(AVG(sm2zone01_39), 4) AS avg_sm2zone01_39, ROUND(AVG(sm2zone01_40), 4) AS avg_sm2zone01_40,
        ROUND(AVG(sm2zone01_41), 4) AS avg_sm2zone01_41, ROUND(AVG(sm2zone01_42), 4) AS avg_sm2zone01_42, ROUND(AVG(sm2zone01_43), 4) AS avg_sm2zone01_43, ROUND(AVG(sm2zone01_44), 4) AS avg_sm2zone01_44, ROUND(AVG(sm2zone01_45), 4) AS avg_sm2zone01_45, ROUND(AVG(sm2zone01_46), 4) AS avg_sm2zone01_46, ROUND(AVG(sm2zone01_47), 4) AS avg_sm2zone01_47, ROUND(AVG(sm2zone01_48), 4) AS avg_sm2zone01_48, ROUND(AVG(sm2zone01_49), 4) AS avg_sm2zone01_49, ROUND(AVG(sm2zone01_50), 4) AS avg_sm2zone01_50,
        ROUND(AVG(sm2zone01_51), 4) AS avg_sm2zone01_51, ROUND(AVG(sm2zone01_52), 4) AS avg_sm2zone01_52, ROUND(AVG(sm2zone01_53), 4) AS avg_sm2zone01_53, ROUND(AVG(sm2zone01_54), 4) AS avg_sm2zone01_54, ROUND(AVG(sm2zone01_55), 4) AS avg_sm2zone01_55, ROUND(AVG(sm2zone01_56), 4) AS avg_sm2zone01_56, ROUND(AVG(sm2zone01_57), 4) AS avg_sm2zone01_57, ROUND(AVG(sm2zone01_58), 4) AS avg_sm2zone01_58, ROUND(AVG(sm2zone01_59), 4) AS avg_sm2zone01_59, ROUND(AVG(sm2zone01_60), 4) AS avg_sm2zone01_60,
        ROUND(AVG(sm2zone01_61), 4) AS avg_sm2zone01_61, ROUND(AVG(sm2zone01_62), 4) AS avg_sm2zone01_62, ROUND(AVG(sm2zone01_63), 4) AS avg_sm2zone01_63, ROUND(AVG(sm2zone01_64), 4) AS avg_sm2zone01_64, ROUND(AVG(sm2zone01_65), 4) AS avg_sm2zone01_65, ROUND(AVG(sm2zone01_66), 4) AS avg_sm2zone01_66, ROUND(AVG(sm2zone01_67), 4) AS avg_sm2zone01_67, ROUND(AVG(sm2zone01_68), 4) AS avg_sm2zone01_68, ROUND(AVG(sm2zone01_69), 4) AS avg_sm2zone01_69, ROUND(AVG(sm2zone01_70), 4) AS avg_sm2zone01_70,
        ROUND(AVG(sm2zone01_71), 4) AS avg_sm2zone01_71, ROUND(AVG(sm2zone01_72), 4) AS avg_sm2zone01_72, ROUND(AVG(sm2zone01_73), 4) AS avg_sm2zone01_73, ROUND(AVG(sm2zone01_74), 4) AS avg_sm2zone01_74, ROUND(AVG(sm2zone01_75), 4) AS avg_sm2zone01_75, ROUND(AVG(sm2zone01_76), 4) AS avg_sm2zone01_76, ROUND(AVG(sm2zone01_77), 4) AS avg_sm2zone01_77, ROUND(AVG(sm2zone01_78), 4) AS avg_sm2zone01_78, ROUND(AVG(sm2zone01_79), 4) AS avg_sm2zone01_79, ROUND(AVG(sm2zone01_80), 4) AS avg_sm2zone01_80,
        ROUND(AVG(sm2zone01_81), 4) AS avg_sm2zone01_81, ROUND(AVG(sm2zone01_82), 4) AS avg_sm2zone01_82, ROUND(AVG(sm2zone01_83), 4) AS avg_sm2zone01_83, ROUND(AVG(sm2zone01_84), 4) AS avg_sm2zone01_84, ROUND(AVG(sm2zone01_85), 4) AS avg_sm2zone01_85, ROUND(AVG(sm2zone01_86), 4) AS avg_sm2zone01_86, ROUND(AVG(sm2zone01_87), 4) AS avg_sm2zone01_87, ROUND(AVG(sm2zone01_88), 4) AS avg_sm2zone01_88, ROUND(AVG(sm2zone01_89), 4) AS avg_sm2zone01_89, ROUND(AVG(sm2zone01_90), 4) AS avg_sm2zone01_90,
